<?xml version="1.0" encoding="utf-8"?>

<urlrewrite>
    <rule match-type="wildcard">
        <from>/post/**</from>
        <to>/post.zul</to>
    </rule>
    
    <rule match-type="wildcard">
        <from>/category/**</from>
        <to>/listpost.zul</to>
    </rule>
    <rule match-type="wildcard">
        <from>/admin</from>
        <to>/backend/index.zul</to>
    </rule>
<!--    <rule match-type="wildcard">
        <from>/</from>
        <to>/newsadvertisement/index.zul</to>
    </rule>-->
</urlrewrite>
