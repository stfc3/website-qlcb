<zk xmlns:x="xhtml" xmlns:zk="zk">    
    <div apply="com.stfc.website.newsadvertisement.ListPostNewsAdvertController">
        
        <include src="/header.zul"/>
        <!-- Main Header end -->
    
    
    
        <!-- Slider -->
        <div class="irs-main-slider">
            <div class="container">
                <x:div id="myCarousel" class="carousel fade-carousel slide" data-ride="carousel" data-interval="3000">
                    <!-- Indicators -->
                    <!-- Wrapper for slides -->
                    <div id="indexSlider" class="carousel-inner">
							
                    </div>
                    <x:ol class="carousel-indicators">
                        <x:li data-target="#myCarousel" data-slide-to="0" class="active"></x:li>
                        <x:li data-target="#myCarousel" data-slide-to="1"></x:li>
                        <x:li data-target="#myCarousel" data-slide-to="2"></x:li>
                    </x:ol>

                </x:div>
            </div>
        </div>
        <!--End Slider -->
        <!-- HOT NEWS -->
        
        <div class="irs-blog-field irs-blog-single-field">
            <div class="container">
                <div class="col-md-9">
                    <div id="listPost"></div>
                </div>
                <div class="col-md-3 widget-right">
                    <div id="addWidgetIndexRight"></div>
                </div>
            </div>
        </div>
        <!-- HOT NEWS end -->

        <!-- Footer start -->
        <div id="addWidgetFooter"></div>
        <!-- Footer end -->

        <include src="/copyright.zul"/>
        
    </div>
        
